<tr>
  <td>
    <%= test.title %>
    <%= render 'form_inline', test: test %>
  </td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to 'Просмотреть', admin_test_path(test) %></td>
  <td><%=  link_to 'Редактировать', edit_admin_test_path(test) %></td>
  <td>
    <%= link_to 'Удалить',
                admin_test_path(test),
                method: :delete,
                data: { confirm: "Вы уверены, что хотите удалить?" } %>
  </td>
</tr>
